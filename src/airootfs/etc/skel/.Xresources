Xcursor.size: 16

!  ROFI

rofi.show-icons:            true
rofi.modi:                  drun,run,window,ssh
rofi.lines:                 5
rofi.line-padding:          7
rofi.matching:              fuzzy
rofi.bw:                    0
rofi.padding:               0
rofi.separator-style:       none
rofi.hide-scrollbar:        true
rofi.line-margin:           0
rofi.font:		    Noto Sans Mono 12	
!rofi.font:                 Hack 14
rofi.theme:		    ~/.config/rofi/config.rasi
